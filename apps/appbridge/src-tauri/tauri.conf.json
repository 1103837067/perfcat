{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "appbridge",
  "version": "0.1.0",
  "identifier": "com.perfcat.appbridge",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.png"
    ]
  },
  "plugins": {}
}
